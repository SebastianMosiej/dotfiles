#!/bin/bash
message "Starting user specific xsession"
lxtask &
insync start &
sleep 1
evolution &
sleep 2
rainlendar2 &
$HOME//bin/Evernote.Linux.Beta.AppImage &
slack &
sleep 1
feh -r $HOME/.wallpaper/Cyberpunk --bg-fill -z -D 300 &
sleep 1
if [ -e /tmp/.displays ]; then
  rm /tmp/.displays
fi
/usr/bin/xscreensaver -no-splash &
/usr/bin/blueman-applet &
/usr/bin/google-chrome-stable &
/usr/bin/google-chrome-stable --user-data-dir="$HOME/.rimac_profile" &

xmind &

xrandr --output DP-1-1-1 --mode 1920x1200 --output DP-1-1-8 --mode 1920x1200 --auto --primary --right-of DP-1-1-1 --output eDP-1 --auto --right-of DP-1-1-8

setxkbmap -option caps:escape
setxkbmap pl
spotify &

#exec xfce4-session
exec i3
